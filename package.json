{
  "name": "legal-data-extraction",
  "version": "1.0.0",
  "description": "Azure OpenAI Batch API system for extracting data from Belgian legal case laws",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "start": "node dist/cli.js",
    "test-decision": "tsx test-decision.ts",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "status": "tsx src/cli.ts status extract-comprehensive",
    "process": "tsx src/cli.ts process extract-comprehensive",
    "list": "tsx src/cli.ts list",
    "eval": "tsx evals/cli.ts",
    "analyze-results": "tsx evals/analyze-cli.ts",
    "analyze-full-data": "tsx scripts/analyze-full-data.ts"
  },
  "keywords": [
    "legal",
    "azure",
    "openai",
    "batch-api",
    "data-extraction"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.67.0",
    "@braintrust/core": "^0.0.35",
    "ajv": "^8.12.0",
    "braintrust": "^0.0.154",
    "dotenv": "^16.4.5",
    "openai": "^4.80.0",
    "p-limit": "^5.0.0",
    "pg": "^8.11.3",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "@types/pg": "^8.11.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  }
}
